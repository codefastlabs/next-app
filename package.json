{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:down": "make database-down",
    "db:up": "make database-up",
    "dev": "next dev --turbopack",
    "postinstall": "pnpm exec simple-git-hooks",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "package:install": "pnpm install",
    "package:reinstall": "rm -rf node_modules .next pnpm-lock.yaml && pnpm install",
    "package:update": "pnpm update --latest --interactive",
    "prettier": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "serve": "next build && next start",
    "start": "next start",
    "test": "vitest --watch=false",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint --edit $1"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 120,
    "singleQuote": true,
    "tailwindFunctions": [
      "tv"
    ]
  },
  "dependencies": {
    "@codefast/hooks": "0.1.58",
    "@codefast/ui": "0.1.58",
    "@hookform/resolvers": "3.9.1",
    "@tanstack/react-query": "5.61.0",
    "@tanstack/react-query-devtools": "5.61.0",
    "@tanstack/react-table": "8.20.5",
    "@vercel/functions": "1.5.0",
    "consola": "3.2.3",
    "date-fns": "4.1.0",
    "jotai": "2.10.3",
    "lodash-es": "4.17.21",
    "lucide-react": "0.460.0",
    "next": "15.0.3",
    "next-themes": "0.4.3",
    "path-to-regexp": "8.2.0",
    "react": "19.0.0-rc-6f0dc294-20241119",
    "react-dom": "19.0.0-rc-6f0dc294-20241119",
    "react-hook-form": "7.53.2",
    "recharts": "2.13.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@codefast/config-tailwind": "0.1.58",
    "@codefast/eslint-config": "0.1.58",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@svgr/webpack": "8.1.0",
    "@tanstack/eslint-plugin-query": "5.60.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.17.6",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@vitejs/plugin-react": "4.3.3",
    "@vitest/coverage-v8": "2.1.5",
    "@vitest/ui": "2.1.5",
    "eslint": "8.57.1",
    "eslint-config-next": "15.0.3",
    "lint-staged": "15.2.10",
    "postcss": "^8.4.49",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.3",
    "prettier-plugin-tailwindcss": "0.6.9",
    "simple-git-hooks": "2.11.1",
    "tailwindcss": "3.4.15",
    "typescript": "5",
    "vitest": "2.1.5"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
      "react": "19.0.0-rc-6f0dc294-20241119",
      "react-dom": "19.0.0-rc-6f0dc294-20241119",
      "react-is": "19.0.0-rc-6f0dc294-20241119"
    }
  }
}
